{
    "name": "nexustrade-trading-engine",
    "version": "1.0.0",
    "description": "High Win Rate Trading Engine with 80%+ Success Rate Strategies",
    "main": "src/tradingEngine.js",
    "scripts": {
        "start": "node src/tradingEngine.js",
        "dev": "nodemon src/tradingEngine.js",
        "test": "NODE_ENV=test jest --detectOpenHandles --forceExit --no-lint",
        "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles",
        "test:coverage": "NODE_ENV=test jest --coverage --detectOpenHandles --coverageReporters=\"text-summary\"",
        "test:coverage:html": "NODE_ENV=test jest --coverage --detectOpenHandles --coverageReporters=html",
        "test:coverage:ci": "NODE_ENV=test jest --ci --coverage --detectOpenHandles --coverageReporters=text-lcov | coveralls",
        "test:api": "NODE_ENV=test jest test/strategyExecutionAPI.test.js --detectOpenHandles --forceExit",
        "test:integration": "NODE_ENV=test jest test/integration --detectOpenHandles --forceExit",
        "test:unit": "NODE_ENV=test jest test/unit --detectOpenHandles --forceExit",
        "test:api:watch": "NODE_ENV=test jest --watch test/strategyExecutionAPI.test.js --detectOpenHandles",
        "test:coverage:report": "npm run test:coverage && open coverage/lcov-report/index.html",
        "test:all": "npm run test:unit && npm run test:integration && npm run test:coverage",
        "benchmark": "node benchmarks/latency-test.js",
        "stress-test": "node benchmarks/stress-test.js",
        "lint": "eslint src test --ext .js",
        "lint:fix": "eslint src test --ext .js --fix",
        "pretest": "echo 'Skipping lint for now' || npm run lint",
        "precommit": "lint-staged"
    },
    "dependencies": {
        "avl": "^1.5.3",
        "axios": "^1.3.4",
        "big.js": "^6.2.1",
        "bull": "^4.10.4",
        "bullmq": "^3.13.3",
        "ccxt": "^3.0.92",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "decimal.js": "^10.4.3",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-validator": "^6.15.0",
        "fast-json-stringify": "^5.7.0",
        "fastest-validator": "^1.17.0",
        "flatbuffers": "^23.3.3",
        "helmet": "^6.1.5",
        "ioredis": "^5.3.1",
        "joi": "^17.9.1",
        "kafkajs": "^2.2.4",
        "lru-cache": "^9.1.1",
        "microtime": "^3.1.1",
        "moment": "^2.29.4",
        "mongoose": "^7.0.3",
        "msgpack5": "^4.5.1",
        "nanoid": "^4.0.2",
        "node-cron": "^3.0.2",
        "priorityqueuejs": "^2.0.0",
        "prom-client": "^14.2.0",
        "redis": "^4.6.5",
        "socket.io": "^4.6.1",
        "uuid": "^9.0.0",
        "winston": "^3.8.2",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/plugin-transform-runtime": "^7.28.0",
        "@babel/preset-env": "^7.21.5",
        "babel-jest": "^29.5.0",
        "benchmark": "^2.1.4",
        "chai": "^4.3.7",
        "chai-http": "^4.3.0",
        "coveralls": "^3.1.1",
        "eslint": "^8.33.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-html-reporter": "^3.8.0",
        "jest-junit": "^16.0.0",
        "jest-sonar-reporter": "^2.0.0",
        "jsonwebtoken": "^9.0.2",
        "lint-staged": "^13.2.0",
        "mocha": "^10.2.0",
        "morgan": "^1.10.0",
        "nock": "^13.2.9",
        "nodemon": "^2.0.22",
        "nyc": "^15.1.0",
        "prettier": "^2.8.8",
        "sinon": "^15.0.1",
        "supertest": "^6.3.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,yml}": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
